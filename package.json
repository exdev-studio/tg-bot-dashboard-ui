{
  "name": "tg-bot-dashboard-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "replace-env-variables": "replace-in-file src/environments/environment.ts --configFile=scripts/replace-env-variables-config.js",
    "prestart": "npm run replace-env-variables",
    "start": "ng serve --open",
    "build": "npm run replace-env-variables && ng build",
    "test": "ng test",
    "generate-svg-sprite": "svgstore INPUT src/assets/icons/*.svg -o src/app/component/svg-sprite/svg-sprite.component.html",
    "postinstall": "npm run generate-svg-sprite"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.1",
    "@angular/common": "~12.1.1",
    "@angular/compiler": "~12.1.1",
    "@angular/core": "~12.1.1",
    "@angular/forms": "~12.1.1",
    "@angular/localize": "~12.1.1",
    "@angular/platform-browser": "~12.1.1",
    "@angular/platform-browser-dynamic": "~12.1.1",
    "@angular/router": "~12.1.1",
    "@auth0/auth0-angular": "^1.5.1",
    "@fontsource/fira-sans": "^4.4.5",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "bootstrap": "^4.5.0",
    "ngx-avatar": "^4.1.0",
    "ngx-spinner": "^12.0.0",
    "reset-css": "5.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.1",
    "@angular/cli": "~12.1.1",
    "@angular/compiler-cli": "~12.1.1",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "replace-in-file": "^6.2.0",
    "svgstore": "3.0.0-2",
    "svgstore-cli": "2.0.0",
    "typescript": "~4.3.2"
  }
}
